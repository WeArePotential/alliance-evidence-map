    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "d3": "^4.13.0",
    "lodash": "^4.17.11",
    "striptags": "^3.1.1",
    "vue": "^2.5.17",
    "vue-multiselect": "^2.1.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.3.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
